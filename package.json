{
    "name": "nba-service",
    "version": "0.0.0",
    "description": "nba service",
    "private": true,
    "main": "index.js",
    "scripts": {
        "build": "tsc",
        "watch": "tsc --watch",
        "start": "tsc && node ./dist/index.js",
        "install:prod": "npm install --production",
        "lint": "eslint -c .eslintrc.json --no-error-on-unmatched-pattern ./src/**/*",
        "lint:fix": "eslint -c .eslintrc.json --no-error-on-unmatched-pattern ./src/**/* --fix",
        "test": "tsc && jest --maxWorkers=1 --testPathPattern .spec.ts$",
        "test:unit": "tsc && jest --testPathPattern .manager.spec.ts$ .repository.spec.ts$ .utils.spec.ts$",
        "test:integration": "tsc && jest --maxWorkers=1 --testPathPattern .router.spec.ts$",
        "coverage": "tsc && jest --coverage",
        "release": "semantic-release"
    },
    "author": "Aniam Wolff",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "FILL_IF_NEEDED"
    },
    "dependencies": {
        "axios": "^0.27.2",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "env-var": "^7.1.1",
        "express": "^4.17.1",
        "helmet": "^4.6.0",
        "jayson": "^4.1.0",
        "joi": "^17.4.2",
        "morgan": "^1.10.0",
        "redis": "^4.6.12",
        "supertest": "^6.2.4"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/jest": "^28.1.6",
        "@types/morgan": "^1.9.3",
        "@types/node": "^16.11.9",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.23.0",
        "@typescript-eslint/parser": "^5.23.0",
        "eslint": "^8.3.0",
        "eslint-config-airbnb-typescript": "^16.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^28.1.3",
        "nock": "^13.2.9",
        "nodemon": "^2.0.15",
        "prettier": "^2.4.1",
        "semantic-release": "^19.0.5",
        "supertest": "^6.1.6",
        "ts-jest": "^28.0.7",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.2"
    }
}
